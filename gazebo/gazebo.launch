<launch>
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find xiaohu_robot)/gazebo/worlds/home.world" />
        <arg name="output" value="log" />
    </include>

    <node name="bed_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/bed.model -x 5.0 -y -3.9 -z 0 -Y 3.14159 -urdf -model bed" />
    <node name="sofa_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/sofa.model -x -1.0 -y -3.9 -z 0 -Y 1.57 -urdf -model sofa" />
    <node name="tea_table_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/tea_table.model -x -2.1 -y -2.2 -z 0 -Y 1.57 -urdf -model tea_table" />
    <node name="bookshelft_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/bookshelft.model -x 2.0 -y -0.55 -z 0 -Y -1.57 -urdf -model bookshelft" />
    <node name="kitchen_table_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/table.model -x -3.5 -y 3.7 -z 0 -Y 1.57 -urdf -model kitchen_table" />
    <node name="cupboard_0_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/cupboard.model -x -2.0 -y 0.7 -z 0 -Y 1.57 -urdf -model cupboard_0" />
    <node name="cupboard_1_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/cupboard.model -x -1.3 -y 3.7 -z 0 -Y -1.57 -urdf -model cupboard_1" />
    <node name="dinning_table_0_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/table.model -x 1.5 -y 1.5 -z 0 -Y 1.57 -urdf -model dinning_table_0" />
    <node name="dinning_table_1_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/table.model -x 1.5 -y 2.0 -z 0 -Y 1.57 -urdf -model dinning_table_1" />
    <node name="dinning_table_2_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/table.model -x 2.7 -y 1.5 -z 0 -Y 1.57 -urdf -model dinning_table_2" />
    <node name="dinning_table_3_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/table.model -x 2.7 -y 2.0 -z 0 -Y 1.57 -urdf -model dinning_table_3" />
    <node name="chair_0_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/chair.model -x 1.5 -y 1.2 -z 0 -Y 1.57 -urdf -model chair_0" />
    <node name="chair_1_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/chair.model -x 1.5 -y 2.3 -z 0 -Y -1.57 -urdf -model chair_1" />
    <node name="chair_2_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/chair.model -x 2.7 -y 1.2 -z 0 -Y 1.57 -urdf -model chair_2" />
    <node name="chair_3_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/chair.model -x 2.7 -y 2.3 -z 0 -Y -1.57 -urdf -model chair_3" />
    <node name="bottle_0_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/bottles/red_bottle.model -x -3.3 -y 3.55 -z 10.0 -Y 0 -urdf -model bottle_0" />
    <node name="bottle_1_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/bottles/green_bottle.model -x -3.6 -y 3.55 -z 10.0 -Y 0 -urdf -model bottle_1" />
    <node name="robot_model" pkg="gazebo_ros" type="spawn_model" launch-prefix="nice" args="-file $(find xiaohu_robot)/gazebo/models/robot.model -urdf -x -0.0270498 -y -3.37887 -Y 1.57 -model robot" />
</launch>