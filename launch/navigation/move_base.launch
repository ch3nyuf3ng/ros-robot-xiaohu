<launch>
    <node pkg="move_base" type="move_base" name="move_base">
        <rosparam file="$(find xiaohu_robot)/launch/navigation/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find xiaohu_robot)/launch/navigation/params/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find xiaohu_robot)/launch/navigation/params/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find xiaohu_robot)/launch/navigation/params/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find xiaohu_robot)/launch/navigation/params/local_planner_params.yaml" command="load" />
        <param name="base_global_planner" value="global_planner/GlobalPlanner" />
        <param name="base_local_planner" value="wpbh_local_planner/WpbhLocalPlanner" />
        <param name= "controller_frequency" value="10" type="double" />
    </node>
</launch>